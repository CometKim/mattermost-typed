};

declare type mattermost$AuthService =
  | ''
  | 'email'
  | 'gitlab'
  | 'google'
  | 'office365'
  | 'ldap'
  | 'saml';

declare type mattermost$Locale = 
  | 'de'
  | 'en'
  | 'es'
